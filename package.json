    "@actions/core": "^1.11.1"
  },
  "devDependencies": {
    "@github/local-action": "^2.5.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^8.57.1",
    "eslint-plugin-github": "^5.1.5",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.4.2"
  }
